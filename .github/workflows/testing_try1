name : testing_workflow1
on: [push]
 # label:
  #  types:
   #   - created
  # push:
  #  branches:
   #   - master

  workflow_call:
    inputs:
      username:
        description: 'A username passed from the caller workflow'
        default: 'jas'
        required: false
        type: string

jobs:
  print-username:
    runs-on: windows-latest

    steps:
      - name: Print the input name to STDOUT
        run: echo The username is ${{ inputs.username }}
